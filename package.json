{
  "name": "event-locator",
  "version": "1.0.0",
  "description": "Multi-user event locator application with geospatial features",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit --config jest.config.js",
    "test:watch": "cross-env NODE_ENV=test jest --watch --runInBand --config jest.config.js",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage --runInBand --forceExit --config jest.config.js",
    "test:verbose": "cross-env NODE_ENV=test jest --verbose --config jest.config.js",
    "lint": "eslint src/**/*.js",
    "migrate": "node src/database/migrations/run-migrations.js",
    "test:reset-db": "cross-env NODE_ENV=test node src/scripts/reset-test-db.js",
    "pretest": "cross-env NODE_ENV=test node tests/db-setup.js"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^6.1.5",
    "i18next": "^22.4.15",
    "i18next-fs-backend": "^2.1.1",
    "i18next-http-middleware": "^3.3.0",
    "ioredis": "^5.6.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "redis": "^4.6.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.8.2",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.4"
  }
}
